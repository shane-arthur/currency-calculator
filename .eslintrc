{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules" : {
        "import/no-unresolved" : 0,
        "react/prop-types" : 0,
        "linebreak-style" : 0,
        "no-console" : 0,
        "indent": 0,
        "max-len": 0,
        "comma-dangle": 0,
        "arrow-parens": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "react/jsx-filename-extension": 0,
        "consistent-return": 0,
        "import/prefer-default-export": 0,
        "valid-typeof": 0,
        "arrow-body-style": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "react/jsx-no-bind": 0,
        "no-script-url" : 0,
        "no-undef": 0,
        "brace-style": 0,
        "no-else-return": 0
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}